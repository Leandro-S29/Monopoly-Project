    <function name="LDLancarDados" type="None" variable="">
        <parameters>
            <parameter name="Jogador" type="String" array="False"/>
            <parameter name="D1" type="Integer" array="False"/>
            <parameter name="D2" type="Integer" array="False"/>
            <parameter name="JogoEmProgresso" type="Boolean" array="False"/>
            <parameter name="JogadoresNoJogo" type="String" array="True"/>
            <parameter name="JaJogouEsteTurno" type="Boolean" array="True"/>
            <parameter name="Duplicados" type="Integer" array="True"/>
            <parameter name="PrecisaPagarRenda" type="Boolean" array="True"/>
            <parameter name="TemCartaComunityOuChance" type="Boolean" array="True"/>
            <parameter name="EstaNaPrisao" type="Boolean" array="True"/>
            <parameter name="TurnosNaPrisao" type="Integer" array="True"/>
            <parameter name="JogadorAtivo" type="Integer" array="False"/>
            <parameter name="Posicao" type="Integer" array="True"/>
        </parameters>
        <body>
            <if expression="JogoEmProgresso != True">
                <then>
                    <output expression="&quot;N&#227;o existe um jogo em curso.&quot;" newline="True"/>
                </then>
                <else>
                    <declare name="EstaNoJogo" type="Boolean" array="False" size=""/>
                    <assign variable="EstaNoJogo" expression="EstaNoJogo(Jogador, JogadoresNoJogo)"/>
                    <if expression="EstaNoJogo == True">
                        <then>
                            <if expression="Jogador == JogadoresNoJogo[JogadorAtivo]">
                                <then>
                                    <if expression="JaJogouEsteTurno[JogadorAtivo] != true &amp;&amp; Duplicados[JogadorAtivo] == 0">
                                        <then>
                                            <if expression="PrecisaPagarRenda[JogadorAtivo] != True || TemCartaComunityOuChance[JogadorAtivo] != True">
                                                <then>
                                                    <if expression="EstaNaPrisao[JogadorAtivo] != True">
                                                        <then>
                                                            <call expression="Movimentacao(d1, d2, posicao, JogadorAtivo)"/>
                                                        </then>
                                                        <else>
                                                            <if expression="d1 == d2">
                                                                <then>
                                                                    <assign variable="EstaNaPrisao" expression="false"/>
                                                                    <assign variable="TurnosNaPrisao" expression="0"/>
                                                                    <call expression="Movimentacao(d1, d2, posicao, JogadorAtivo)"/>
                                                                </then>
                                                                <else>
                                                                    <assign variable="TurnosNaPrisao" expression="TurnosNaPrisao + 1"/>
                                                                    <assign variable="JaJogouEsteturno" expression="True"/>
                                                                    <output expression="&quot;Saiu &quot; + d1 +&quot;/&quot; + d2 + &quot; - espa&#231;o Prison. Jogador s&#243; de passagem&quot;" newline="True"/>
                                                                </else>
                                                            </if>
                                                        </else>
                                                    </if>
                                                </then>
                                                <else/>
                                            </if>
                                        </then>
                                        <else>
                                            <output expression="&quot;N&#227;o &#233; a vez do Jogador.&quot;" newline="True"/>
                                        </else>
                                    </if>
                                </then>
                                <else>
                                    <output expression="&quot;N&#227;o &#233; o turno deste jogador.&quot;" newline="True"/>
                                </else>
                            </if>
                        </then>
                        <else>
                            <output expression="&quot;Jogador n&#227;o participa do jogo em curso&quot;" newline="True"/>
                        </else>
                    </if>
                </else>
            </if>
        </body>
    </function>
    <function name="LJListarJogadores" type="None" variable="">
        <parameters>
            <parameter name="Jogadores" type="String" array="True"/>
        </parameters>
        <body>
            <if expression="jogadores[0] == &quot;&quot;">
                <then>
                    <output expression="&quot;Sem jogadores registados.&quot;" newline="True"/>
                </then>
                <else>
                    <declare name="tempNome" type="String" array="False" size=""/>
                    <declare name="tempVit" type="Integer" array="False" size=""/>
                    <declare name="Vitorias" type="Integer" array="True" size="Size(jogadores)"/>
                    <declare name="i" type="Integer" array="False" size=""/>
                    <for variable="i" start="0" end="size(jogadores) - 1" direction="inc" step="1">
                        <assign variable="Vitorias[i]" expression="Random(8)"/>
                    </for>
                    <declare name="h" type="Integer" array="False" size=""/>
                    <for variable="h" start="0" end="size(jogadores) - 1" direction="inc" step="1">
                        <declare name="j" type="Integer" array="False" size=""/>
                        <for variable="j" start="0" end="Size(Jogadores) - 2" direction="inc" step="1">
                            <if expression="(Vitorias[j] &lt; Vitorias[j+1]) || (Vitorias[j] == Vitorias[j+1] &amp;&amp; Jogadores[j] &gt; Jogadores[j+1])">
                                <then>
                                    <assign variable="tempVit" expression="Vitorias[j]"/>
                                    <assign variable="Vitorias[j]" expression="Vitorias[j+1]"/>
                                    <assign variable="Vitorias[j+1]" expression="tempVit"/>
                                    <assign variable="tempNome" expression="jogadores[j]"/>
                                    <assign variable="jogadores[j]" expression="jogadores[j+1]"/>
                                    <assign variable="jogadores[j+1]" expression="tempNome"/>
                                </then>
                                <else/>
                            </if>
                        </for>
                    </for>
                    <declare name="k" type="Integer" array="False" size=""/>
                    <for variable="k" start="0" end="Size(jogadores) - 1" direction="inc" step="1">
                        <output expression="Jogadores[k] &amp; &quot; &quot; &amp; Vitorias[k] &amp; &quot; &quot; &amp; Random(4) &amp; &quot; &quot; &amp; Random(6) &amp; &quot; &quot; &amp; Random(10)" newline="True"/>
                    </for>
                </else>
            </if>
        </body>
    </function>
    <function name="Movimentacao" type="None" variable="">
        <parameters>
            <parameter name="d1" type="Integer" array="False"/>
            <parameter name="d2" type="Integer" array="False"/>
            <parameter name="Posicao" type="Integer" array="True"/>
            <parameter name="JogadorAtivo" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="BordaEsquerda" type="Integer" array="True" size="7"/>
            <assign variable="BordaEsquerda[0]" expression="0"/>
            <declare name="BordaDireita" type="Integer" array="True" size="7"/>
            <assign variable="BordaDireita[0]" expression="6"/>
            <declare name="i" type="Integer" array="False" size=""/>
            <for variable="i" start="0" end="size(BordaEsquerda) - 1" direction="inc" step="1">
                <assign variable="BordaEsquerda[i]" expression="0"/>
                <assign variable="BordaDireita[i]" expression="0"/>
            </for>
            <assign variable="BordaDireita[0]" expression="6"/>
            <for variable="i" start="1" end="size(BordaEsquerda)-1" direction="inc" step="1">
                <assign variable="BordaEsquerda[i]" expression="BordaEsquerda[i] + 7"/>
                <assign variable="BordaDireita[i]" expression="BordaDireita[i] + 7"/>
            </for>
            <declare name="MovimentoLinhas" type="Integer" array="False" size=""/>
            <declare name="MovimentoColunas" type="Integer" array="False" size=""/>
            <assign variable="MovimentoLinhas" expression="d1"/>
            <assign variable="MovimentoColunas" expression="7 * d2"/>
            <output expression="MovimentoColunas &amp; &quot;gfire&quot;" newline="True"/>
            <declare name="NovaPosicao" type="Integer" array="False" size=""/>
            <assign variable="NovaPosicao" expression="Posicao[JogadorAtivo]"/>
            <output expression="&quot;Posicao:&quot; &amp; NovaPosicao" newline="True"/>
            <declare name="LinhaAtual" type="Integer" array="False" size=""/>
            <assign variable="LinhaAtual" expression="posicao[JogadorAtivo]/7"/>
            <output expression="LinhaAtual" newline="True"/>
            <output expression="Posicao[JogadorAtivo]" newline="True"/>
            <assign variable="NovaPosicao" expression="Posicao[JogadorAtivo]+MovimentoLinhas"/>
            <output expression="NovaPosicao" newline="True"/>
            <if expression="NovaPosicao &lt; BordaEsquerda[LinhaAtual + 1]">
                <then>
                    <assign variable="NovaPosicao" expression="NovaPosicao + 7"/>
                    <output expression="&quot;supertrue&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="NovaPosicao &gt; BordaDireita[LinhaAtual + 1]">
                        <then>
                            <assign variable="NovaPosicao" expression="NovaPosicao - 7"/>
                            <output expression="&quot;supertrue2&quot;" newline="True"/>
                        </then>
                        <else/>
                    </if>
                </else>
            </if>
            <assign variable="NovaPosicao" expression="NovaPosicao - MovimentoColunas"/>
            <if expression="NovaPosicao &lt; 0">
                <then>
                    <assign variable="NovaPosicao" expression="NovaPosicao + 49"/>
                    <output expression="True" newline="True"/>
                </then>
                <else>
                    <if expression="NovaPosicao &gt; 48">
                        <then>
                            <assign variable="NovaPosicao" expression="NovaPosicao -  49"/>
                            <output expression="&quot;true2&quot;" newline="True"/>
                        </then>
                        <else/>
                    </if>
                </else>
            </if>
            <assign variable="Posicao[JogadorAtivo]" expression="NovaPosicao"/>
            <output expression="Posicao[JogadorAtivo]" newline="True"/>
        </body>
    </function>
