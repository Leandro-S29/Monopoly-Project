    <function name="DJDetalhesDoJogo" type="None" variable="">
        <parameters>
            <parameter name="Tabuleiro" type="String" array="True"/>
            <parameter name="Jogadores" type="String" array="True"/>
            <parameter name="Peoes" type="Integer" array="True"/>
        </parameters>
        <body>
            <if expression="JogoEmProgresso = True">
                <then>
                    <declare name="i" type="Integer" array="False" size=""/>
                    <for variable="i" start="0" end="Size(Tabuleiro) - 1" direction="inc" step="1">
                        <output expression="&quot;| &quot; &amp; Tabuleiro[i] &amp; &quot; &quot;" newline="False"/>
                        <if expression="Dono[i] != &quot;&quot;">
                            <then>
                                <if expression="NumeroDeCasas[i] &gt; 0">
                                    <then>
                                        <output expression="&quot;(&quot; &amp; Dono[i] &amp; &quot; - &quot; &amp; NumeroDeCasas[i] &amp; &quot; casas) &quot;" newline="False"/>
                                    </then>
                                    <else>
                                        <output expression="&quot;(&quot; &amp; Dono[i] &amp; &quot;) &quot;" newline="False"/>
                                    </else>
                                </if>
                            </then>
                            <else>
                                <output expression="&quot; &quot;" newline="False"/>
                            </else>
                        </if>
                        <declare name="j" type="Integer" array="False" size=""/>
                        <for variable="j" start="0" end="Size(Peoes) - 1" direction="inc" step="1">
                            <if expression="Peoes[j] = i">
                                <then>
                                    <output expression="Jogadores[j] &amp; &quot; &quot;" newline="False"/>
                                </then>
                                <else/>
                            </if>
                        </for>
                        <if expression="(i + 1) % 7 = 0">
                            <then>
                                <output expression="&quot;|&quot;" newline="True"/>
                            </then>
                            <else/>
                        </if>
                    </for>
                    <output expression="Jogadores[JogadorAtivo] &amp; &quot; - &quot; &amp; Dinheiro[JogadorAtivo]" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;N&#227;o existe jogo em curso&quot;" newline="True"/>
                </else>
            </if>
        </body>
    </function>
    <function name="EstaNoJogo" type="Boolean" variable="estaNoJogo">
        <parameters>
            <parameter name="Jogador" type="String" array="False"/>
            <parameter name="JogadoresNoJogo" type="String" array="True"/>
        </parameters>
        <body>
            <declare name="estaNoJogo" type="Boolean" array="False" size=""/>
            <assign variable="estaNoJogo" expression="false"/>
            <declare name="i" type="Integer" array="False" size=""/>
            <for variable="i" start="0" end="size(JogadoresNoJogo)-1" direction="inc" step="1">
                <if expression="Jogador == JogadoresNoJogo[i]">
                    <then>
                        <assign variable="estaNoJogo" expression="True"/>
                    </then>
                    <else/>
                </if>
            </for>
        </body>
    </function>
    <function name="IJIniciarJogo" type="Boolean" variable="JogoEmProgresso">
        <parameters>
            <parameter name="Jogadores" type="String" array="True"/>
            <parameter name="ActiveInGamePlayers" type="String" array="True"/>
            <parameter name="JogoEmProgresso" type="Boolean" array="False"/>
        </parameters>
        <body>
            <if expression="JogoEmProgresso == true">
                <then>
                    <output expression="&quot;Existe um jogo em curso.&quot;" newline="True"/>
                </then>
                <else>
                    <declare name="NumJogadores" type="Integer" array="False" size=""/>
                    <assign variable="NumJogadores" expression="0"/>
                    <declare name="f" type="Integer" array="False" size=""/>
                    <for variable="f" start="0" end="size(Jogadores)- 1" direction="inc" step="1">
                        <if expression="Jogadores[f] == &quot;&quot;">
                            <then/>
                            <else>
                                <assign variable="NumJogadores" expression="NumJogadores + 1"/>
                            </else>
                        </if>
                    </for>
                    <if expression="NumJogadores == 4">
                        <then>
                            <output expression="&quot;Jogo Iniciado com sucesso&quot;" newline="True"/>
                            <assign variable="JogoEmProgresso" expression="True"/>
                            <declare name="i" type="Integer" array="False" size=""/>
                            <for variable="i" start="0" end="3" direction="inc" step="1">
                                <assign variable="ActiveInGamePlayers[i]" expression="Jogadores[i]"/>
                            </for>
                        </then>
                        <else>
                            <output expression="&quot;Jogador inexistente.&quot;" newline="True"/>
                        </else>
                    </if>
                </else>
            </if>
        </body>
    </function>
