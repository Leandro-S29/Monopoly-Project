        <body>
            <if expression="JogoEmProgresso != True">
                <then>
                    <output expression="&quot;N&#227;o existe um jogo em curso.&quot;" newline="True"/>
                </then>
                <else>
                    <declare name="EstaNoJogo" type="Boolean" array="False" size=""/>
                    <assign variable="EstaNoJogo" expression="EstaNoJogo(Jogador, JogadoresNoJogo)"/>
                    <if expression="EstaNoJogo == True">
                        <then>
                            <if expression="Jogador == JogadoresNoJogo[JogadorAtivo]">
                                <then>
                                    <if expression="JaJogouEsteTurno[JogadorAtivo] == False">
                                        <then>
                                            <if expression="PrecisaPagarRenda[JogadorAtivo] != True || TemCartaComunityOuChance[JogadorAtivo] != True">
                                                <then>
                                                    <if expression="EstaNaPrisao[JogadorAtivo] != True">
                                                        <then>
                                                            <if expression="d1 == d2">
                                                                <then>
                                                                    <if expression="SegundaVez[JogadorAtivo] == True">
                                                                        <then>
                                                                            <assign variable="Posicao[JogadorAtivo]" expression="0"/>
                                                                            <assign variable="TurnosNaPrisao[JogadorAtivo]" expression="0"/>
                                                                            <assign variable="EstaNaPrisao[JogadorAtivo]" expression="True"/>
                                                                            <assign variable="JaJogouEsteturno[JogadorAtivo]" expression="True"/>
                                                                            <assign variable="SegundaVez[JogadorAtivo]" expression="False"/>
                                                                        </then>
                                                                        <else>
                                                                            <call expression="Movimentacao(d1, d2, posicao, JogadorAtivo)"/>
                                                                            <call expression="LDOutputs(Posicao, d1, d2, Tabuleiro, Tipo, JogadorAtivo, Dono, JogadoresNoJogo, TurnosNaPrisao, EstaNaPrisao, DinheiroFreePark, Dinheiro, TemCartaCommunityOuChance, PrecisaPagarRenda)"/>
                                                                            <assign variable="SegundaVez[JogadorAtivo]" expression="True"/>
                                                                        </else>
                                                                    </if>
                                                                </then>
                                                                <else>
                                                                    <call expression="Movimentacao(d1, d2, posicao, JogadorAtivo)"/>
                                                                    <call expression="LDOutputs(Posicao, d1, d2, Tabuleiro, Tipo, JogadorAtivo, Dono, JogadoresNoJogo, TurnosNaPrisao, EstaNaPrisao, DinheiroFreePark, Dinheiro, TemCartaCommunityOuChance, PrecisaPagarRenda)"/>
                                                                    <assign variable="JaJogouEsteturno[JogadorAtivo]" expression="True"/>
                                                                </else>
                                                            </if>
                                                        </then>
                                                        <else>
                                                            <if expression="d1 == d2">
                                                                <then>
                                                                    <assign variable="EstaNaPrisao[JogadorAtivo]" expression="false"/>
                                                                    <assign variable="TurnosNaPrisao[JogadorAtivo]" expression="0"/>
                                                                    <assign variable="JaJogouEsteturno[JogadorAtivo]" expression="False"/>
                                                                </then>
                                                                <else>
                                                                    <assign variable="TurnosNaPrisao[JogadorAtivo]" expression="TurnosNaPrisao[JogadorAtivo] + 1"/>
                                                                    <assign variable="JaJogouEsteturno[JogadorAtivo]" expression="True"/>
                                                                    <if expression="TurnosNaPrisao[JogadorAtivo] == 3">
                                                                        <then>
                                                                            <assign variable="EstaNaPrisao[JogadorAtivo]" expression="False"/>
                                                                        </then>
                                                                        <else/>
                                                                    </if>
                                                                </else>
                                                            </if>
                                                        </else>
                                                    </if>
                                                </then>
                                                <else/>
                                            </if>
                                        </then>
                                        <else>
                                            <output expression="&quot;N&#227;o &#233; a vez do Jogador.&quot;" newline="True"/>
                                        </else>
                                    </if>
                                </then>
                                <else>
                                    <output expression="&quot;N&#227;o &#233; o turno deste jogador.&quot;" newline="True"/>
                                </else>
                            </if>
                        </then>
                        <else>
                            <output expression="&quot;Jogador n&#227;o participa do jogo em curso&quot;" newline="True"/>
                        </else>
                    </if>
                </else>
            </if>
        </body>
    </function>
    <function name="LDOutputs" type="None" variable="">
        <parameters>
            <parameter name="Posicao" type="Integer" array="True"/>
            <parameter name="D1" type="Integer" array="False"/>
            <parameter name="D2" type="Integer" array="False"/>
            <parameter name="Tabuleiro" type="String" array="True"/>
            <parameter name="Tipo" type="String" array="True"/>
            <parameter name="JogadorAtivo" type="Integer" array="False"/>
            <parameter name="Dono" type="String" array="True"/>
            <parameter name="JogadoresNoJogo" type="String" array="True"/>
            <parameter name="TurnosNaPrisao" type="Integer" array="True"/>
            <parameter name="EstaNaPrisao" type="Boolean" array="True"/>
            <parameter name="DinheiroFreePark" type="Integer" array="False"/>
            <parameter name="Dinheiro" type="Real" array="True"/>
            <parameter name="TemCartaCommunityOuChance" type="Boolean" array="True"/>
            <parameter name="PrecisaPagarRenda" type="Boolean" array="True"/>
        </parameters>
        <body>
            <output expression="&quot;Saiu &quot; &amp; D1 &amp; &quot;/&quot; &amp; D2 &amp; &quot; - Espa&#231;o &quot; &amp; Tabuleiro[Posicao[JogadorAtivo]] &amp;&quot;  &quot;" newline="False"/>
            <if expression="Tipo[Posicao[JogadorAtivo]] == &quot;Chance&quot; || Tipo[Posicao[JogadorAtivo]] == &quot;Community&quot;">
                <then>
                    <output expression="&quot;Espa&#231;o especial. Tirar carta.&quot;" newline="False"/>
                    <assign variable="TemCartaCommunityOuChance[JogadorAtivo]" expression="True"/>
                </then>
                <else>
                    <if expression="Tipo[Posicao[JogadorAtivo]] == &quot;BackToStart&quot;">
                        <then>
                            <output expression="&quot;Pe&#231;a colocada no espa&#231;o Start&quot;" newline="False"/>
                            <assign variable="Posicao[JogadorAtivo]" expression="24"/>
                        </then>
                        <else>
                            <if expression="Tipo[Posicao[JogadorAtivo]] == &quot;Police&quot;">
                                <then>
                                    <output expression="&quot;Jogador preso&quot;" newline="False"/>
                                    <assign variable="Posicao[JogadorAtivo]" expression="0"/>
                                    <assign variable="TurnosNaPrisao[JogadorAtivo]" expression="0"/>
                                    <assign variable="EstaNaPrisao[JogadorAtivo]" expression="True"/>
                                </then>
                                <else>
                                    <if expression="Tipo[Posicao[JogadorAtivo]] == &quot;Prison&quot;">
                                        <then>
                                            <output expression="&quot;Jogador s&#243; de passagem&quot;" newline="False"/>
                                        </then>
                                        <else>
                                            <if expression="Tipo[Posicao[JogadorAtivo]] == &quot;FreePark&quot;">
                                                <then>
                                                    <output expression="&quot;Jogador recebe ValorGuardadoNoFreePark.&quot;" newline="False"/>
                                                    <assign variable="Dinheiro[JogadorAtivo]" expression="Dinheiro[JogadorAtivo] + DinheiroFreePark"/>
                                                </then>
                                                <else>
                                                    <if expression="Tipo[Posicao[JogadorAtivo]] == &quot;Lux Tax&quot;">
                                                        <then>
                                                            <if expression="Dinheiro[JogadorAtivo] &gt; 80">
                                                                <then>
                                                                    <assign variable="Dinheiro[JogadorAtivo]" expression="Dinheiro[JogadorAtivo] - 80"/>
                                                                    <assign variable="DinheiroFreePark" expression="DinheiroFreePark + 80"/>
                                                                </then>
                                                                <else>
                                                                    <assign variable="Dinheiro[JogadorAtivo]" expression="0"/>
                                                                    <call expression="VerificarFalencia(JogadorAtivo, Dinheiro, JogadoresNoJogo, Dono)"/>
                                                                </else>
                                                            </if>
                                                        </then>
                                                        <else>
                                                            <if expression="Tipo[Posicao[JogadorAtivo]] == &quot;Start&quot;">
                                                                <then>
                                                                    <assign variable="Dinheiro[JogadorAtivo]" expression="Dinheiro[JogadorAtivo] + 200"/>
                                                                </then>
                                                                <else>
                                                                    <if expression="Dono[Posicao[JogadorAtivo]] == &quot;&quot;">
                                                                        <then>
                                                                            <output expression="&quot;Espa&#231;o sem dono&quot;" newline="False"/>
                                                                        </then>
                                                                        <else>
                                                                            <if expression="Dono[Posicao[JogadorAtivo]] == JogadoresNoJogo[JogadorAtivo]">
                                                                                <then>
                                                                                    <output expression="&quot;Espa&#231;o J&#225; Comprado&quot;" newline="False"/>
                                                                                </then>
                                                                                <else>
                                                                                    <output expression="&quot;Espa&#231;o ja comprado por outro jogador. Necess&#225;rio pagar renda.&quot;" newline="False"/>
                                                                                    <assign variable="PrecisaPagarRenda[JogadorAtivo]" expression="True"/>
                                                                                </else>
                                                                            </if>
                                                                        </else>
                                                                    </if>
                                                                </else>
                                                            </if>
                                                        </else>
                                                    </if>
                                                </else>
                                            </if>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
            <output expression="&quot;&quot;" newline="True"/>
        </body>
    </function>
    <function name="LJListarJogadores" type="None" variable="">
        <parameters>
            <parameter name="Jogadores" type="String" array="True"/>
            <parameter name="Vitorias" type="Integer" array="True"/>
        </parameters>
        <body>
            <if expression="jogadores[0] == &quot;&quot;">
                <then>
                    <output expression="&quot;Sem jogadores registados.&quot;" newline="True"/>
                </then>
                <else>
                    <declare name="tempNome" type="String" array="False" size=""/>
                    <declare name="tempVit" type="Integer" array="False" size=""/>
                    <declare name="h" type="Integer" array="False" size=""/>
                    <for variable="h" start="0" end="size(jogadores) - 1" direction="inc" step="1">
                        <declare name="j" type="Integer" array="False" size=""/>
                        <for variable="j" start="0" end="Size(Jogadores) - 2" direction="inc" step="1">
                            <if expression="(Vitorias[j] &lt; Vitorias[j+1]) || (Vitorias[j] == Vitorias[j+1] &amp;&amp; Jogadores[j] &gt; Jogadores[j+1])">
                                <then>
                                    <assign variable="tempVit" expression="Vitorias[j]"/>
                                    <assign variable="Vitorias[j]" expression="Vitorias[j+1]"/>
                                    <assign variable="Vitorias[j+1]" expression="tempVit"/>
                                    <assign variable="tempNome" expression="jogadores[j]"/>
                                    <assign variable="jogadores[j]" expression="jogadores[j+1]"/>
                                    <assign variable="jogadores[j+1]" expression="tempNome"/>
                                </then>
                                <else/>
                            </if>
                        </for>
                    </for>
                    <declare name="k" type="Integer" array="False" size=""/>
                    <for variable="k" start="0" end="Size(jogadores) - 1" direction="inc" step="1">
                        <output expression="Jogadores[k] &amp; &quot; &quot; &amp; Vitorias[k] &amp; &quot; &quot; &amp; Random(4) &amp; &quot; &quot; &amp; Random(6) &amp; &quot; &quot; &amp; Random(10)" newline="True"/>
                    </for>
                </else>
            </if>
        </body>
    </function>
    <function name="Movimentacao" type="None" variable="">
        <parameters>
            <parameter name="d1" type="Integer" array="False"/>
            <parameter name="d2" type="Integer" array="False"/>
            <parameter name="Posicao" type="Integer" array="True"/>
            <parameter name="JogadorAtivo" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="BordaEsquerda" type="Integer" array="True" size="7"/>
            <assign variable="BordaEsquerda[0]" expression="0"/>
            <declare name="BordaDireita" type="Integer" array="True" size="7"/>
            <assign variable="BordaDireita[0]" expression="6"/>
            <declare name="i" type="Integer" array="False" size=""/>
            <for variable="i" start="0" end="size(BordaEsquerda) - 1" direction="inc" step="1">
                <assign variable="BordaEsquerda[i]" expression="0"/>
                <assign variable="BordaDireita[i]" expression="0"/>
            </for>
            <assign variable="BordaDireita[0]" expression="6"/>
            <for variable="i" start="1" end="size(BordaEsquerda)-1" direction="inc" step="1">
                <assign variable="BordaEsquerda[i]" expression="BordaEsquerda[i-1] + 7"/>
                <assign variable="BordaDireita[i]" expression="BordaDireita[i-1] + 7"/>
            </for>
            <declare name="MovimentoLinhas" type="Integer" array="False" size=""/>
            <declare name="MovimentoColunas" type="Integer" array="False" size=""/>
            <assign variable="MovimentoLinhas" expression="d1"/>
            <assign variable="MovimentoColunas" expression="7 * d2"/>
            <declare name="NovaPosicao" type="Integer" array="False" size=""/>
            <assign variable="NovaPosicao" expression="Posicao[JogadorAtivo]"/>
            <declare name="LinhaAtual" type="Integer" array="False" size=""/>
            <assign variable="LinhaAtual" expression="Posicao[JogadorAtivo]/7"/>
            <assign variable="NovaPosicao" expression="Posicao[JogadorAtivo]+MovimentoLinhas"/>
            <if expression="NovaPosicao &lt; BordaEsquerda[LinhaAtual]">
                <then>
                    <assign variable="NovaPosicao" expression="NovaPosicao + 7"/>
                </then>
                <else>
                    <if expression="NovaPosicao &gt; BordaDireita[LinhaAtual]">
                        <then>
                            <assign variable="NovaPosicao" expression="NovaPosicao - 7"/>
                        </then>
                        <else/>
                    </if>
                </else>
            </if>
            <assign variable="NovaPosicao" expression="NovaPosicao - MovimentoColunas"/>
            <if expression="NovaPosicao &lt; 0">
                <then>
                    <assign variable="NovaPosicao" expression="NovaPosicao + 49"/>
                </then>
                <else>
                    <if expression="NovaPosicao &gt; 48">
                        <then>
                            <assign variable="NovaPosicao" expression="NovaPosicao -  49"/>
                        </then>
                        <else/>
                    </if>
                </else>
            </if>
            <assign variable="Posicao[JogadorAtivo]" expression="NovaPosicao"/>
        </body>
    </function>
    <function name="PAPagaroAluguer" type="None" variable="">
        <parameters>
            <parameter name="JogadoresNoJogo" type="String" array="True"/>
            <parameter name="Dinheiro" type="Real" array="True"/>
            <parameter name="JogadorAtivo" type="Integer" array="False"/>
            <parameter name="Posicao" type="Integer" array="True"/>
            <parameter name="Dono" type="String" array="True"/>
            <parameter name="Preco" type="Real" array="True"/>
            <parameter name="NumeroDeCasas" type="Real" array="True"/>
            <parameter name="PrecisaPagarRenda" type="Boolean" array="True"/>
            <parameter name="JogoEmProgresso" type="Boolean" array="False"/>
            <parameter name="Jogador" type="String" array="False"/>
            <parameter name="Tabuleiro" type="String" array="True"/>
        </parameters>
        <body>
            <declare name="Renda" type="Integer" array="False" size=""/>
            <declare name="NomeDono" type="String" array="False" size=""/>
            <declare name="IndiceDono" type="Integer" array="False" size=""/>
            <assign variable="IndiceDono" expression="0"/>
            <declare name="LocalAtual" type="Integer" array="False" size=""/>
            <assign variable="LocalAtual" expression="Posicao[JogadorAtivo]"/>
            <assign variable="NomeDono" expression="Dono[LocalAtual]"/>
            <if expression="JogoEmProgresso == True">
                <then>
                    <if expression="EstaNoJogo(Jogador, JogadoresNoJogo) == True">
                        <then>
                            <if expression="ValidarTurnoDoJogador(Jogador, JogadoresNoJogo, JogadorAtivo)">
                                <then>
                                    <if expression="NomeDono != &quot;&quot; &amp;&amp; NomeDono != JogadoresNoJogo[JogadorAtivo]">
                                        <then>
                                            <assign variable="Renda" expression="CalcularRenda(Preco, NumeroDeCasas, JogadorAtivo, Posicao)"/>
                                            <assign variable="PrecisaPagarRenda[JogadorAtivo]" expression="True"/>
                                            <if expression="Dinheiro[JogadorAtivo] &gt;= Renda">
                                                <then>
                                                    <declare name="i" type="Integer" array="False" size=""/>
                                                    <for variable="i" start="0" end="Size(JogadoresNoJogo) - 1" direction="inc" step="1">
                                                        <if expression="JogadoresNoJogo[i] == NomeDono">
                                                            <then>
                                                                <assign variable="IndiceDono" expression="i"/>
                                                            </then>
                                                            <else/>
                                                        </if>
                                                    </for>
                                                    <assign variable="Dinheiro[JogadorAtivo]" expression="Dinheiro[JogadorAtivo] - Renda"/>
                                                    <assign variable="Dinheiro[IndiceDono]" expression="Dinheiro[IndiceDono] + Renda"/>
                                                    <assign variable="PrecisaPagarRenda[JogadorAtivo]" expression="False"/>
                                                    <output expression="&quot;Aluguer pago.&quot;" newline="True"/>
                                                </then>
                                                <else>
                                                    <output expression="&quot;O jogador n&#227;o tem dinheiro suficiente.&quot;" newline="True"/>
                                                    <assign variable="Dinheiro[JogadorAtivo]" expression="0"/>
                                                    <call expression="VerificarFalencia(JogadorAtivo, Dinheiro, JogadoresNoJogo, Dono, Tabuleiro)"/>
                                                    <assign variable="PrecisaPagarRenda[JogadorAtivo]" expression="False"/>
                                                </else>
                                            </if>
                                        </then>
                                        <else>
                                            <output expression="&quot;N&#227;o &#233; necess&#225;rio pagar aluguer.&quot;" newline="True"/>
                                        </else>
                                    </if>
                                </then>
                                <else>
                                    <output expression="&quot;N&#227;o &#233; a vez do jogador.&quot;" newline="True"/>
                                </else>
                            </if>
                        </then>
                        <else>
                            <output expression="&quot;Jogador n&#227;o participa no jogo em curso.&quot;" newline="True"/>
                        </else>
                    </if>
                </then>
                <else>
                    <output expression="&quot;N&#227;o existe um jogo em curso.&quot;" newline="True"/>
                </else>
            </if>
        </body>
    </function>
    <function name="RJRegistarJogador" type="None" variable="">
        <parameters>
            <parameter name="Jogadores" type="String" array="True"/>
            <parameter name="Indice" type="Integer" array="False"/>
            <parameter name="Nome" type="String" array="False"/>
        </parameters>
        <body>
            <declare name="i" type="Integer" array="False" size=""/>
            <declare name="Existe" type="Boolean" array="False" size=""/>
            <assign variable="Existe" expression="False"/>
            <for variable="i" start="0" end="Size(Jogadores) - 1" direction="inc" step="1">
                <if expression="Jogadores[i] = Nome">
                    <then>
                        <output expression="&quot;Jogador existente.&quot;" newline="True"/>
                        <assign variable="Existe" expression="True"/>
                    </then>
                    <else/>
                </if>
            </for>
            <if expression="Existe">
                <then/>
                <else>
                    <assign variable="Jogadores[Indice]" expression="Nome"/>
                    <output expression="&quot;Jogador registado com sucesso.&quot;" newline="True"/>
                </else>
            </if>
