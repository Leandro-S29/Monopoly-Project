    <function name="CalcularRenda" type="Integer" variable="ValorTotal">
        <parameters>
            <parameter name="Preco" type="Real" array="True"/>
            <parameter name="NumeroDeCasa" type="Real" array="True"/>
            <parameter name="JogadorAtivo" type="Integer" array="False"/>
            <parameter name="Posicao" type="Integer" array="True"/>
        </parameters>
        <body>
            <declare name="RendaBase" type="Real" array="False" size=""/>
            <assign variable="RendaBase" expression="Preco[Posicao[JogadorAtivo]]*0.25"/>
            <declare name="RendaDaCasa" type="Real" array="False" size=""/>
            <assign variable="RendaDaCasa" expression="Preco[Posicao[JogadorAtivo]]*0.75*NumeroDeCasa[Posicao[JogadorAtivo]]"/>
            <declare name="ValorTotal" type="Real" array="False" size=""/>
            <assign variable="ValorTotal" expression="RendaBase + RendaDaCasa"/>
        </body>
    </function>
    <function name="CCComprarCasa" type="None" variable="">
        <parameters>
            <parameter name="JogoEmProgresso" type="Boolean" array="False"/>
            <parameter name="JogadoresNoJogo" type="String" array="True"/>
            <parameter name="Jogador" type="String" array="False"/>
            <parameter name="JogadorAtivo" type="Integer" array="False"/>
            <parameter name="Posicao" type="Integer" array="True"/>
            <parameter name="Tabuleiro" type="String" array="True"/>
            <parameter name="TipoCor" type="String" array="True"/>
            <parameter name="Tipo" type="String" array="True"/>
            <parameter name="Cor" type="String" array="False"/>
            <parameter name="Dono" type="String" array="True"/>
            <parameter name="Dinheiro" type="Real" array="True"/>
            <parameter name="NumeroDeCasas" type="Real" array="True"/>
            <parameter name="Preco" type="Real" array="True"/>
            <parameter name="PosicaoComprarCasa" type="String" array="False"/>
        </parameters>
        <body>
            <if expression="JogoEmProgresso">
                <then>
                    <if expression="EstaNoJogo(Jogador, JogadoresNoJogo)">
                        <then>
                            <if expression="ValidarTurnoDoJogador(Jogador, JogadoresNoJogo, JogadorAtivo)">
                                <then>
                                    <if expression="Tabuleiro[Posicao[JogadorAtivo]] == PosicaoComprarCasa">
                                        <then>
                                            <declare name="CorDaPosicao" type="String" array="False" size=""/>
                                            <assign variable="CorDaPosicao" expression="TipoCor[Posicao[JogadorAtivo]]"/>
                                            <if expression="Tipo[Posicao[JogadorAtivo]] == &quot;Street&quot;">
                                                <then>
                                                    <if expression="TemTodosOsEspacosCores(JogadoresNoJogo[JogadorAtivo],Cor,TipoCor,Dono,Tabuleiro)">
                                                        <then>
                                                            <if expression="NumeroDeCasas[Posicao[JogadorAtivo]] &gt;= 4">
                                                                <then>
                                                                    <output expression="&quot;N&#250;mero m&#225;ximo de casas atingido.&quot;" newline="True"/>
                                                                </then>
                                                                <else>
                                                                    <declare name="ValorDaCasa" type="Real" array="False" size=""/>
                                                                    <assign variable="ValorDaCasa" expression="CustoDaCasa(Preco,Posicao,JogadorAtivo)"/>
                                                                    <if expression="Dinheiro[JogadorAtivo] &lt; ValorDaCasa">
                                                                        <then>
                                                                            <output expression="&quot;O Jogador n&#227;o possui dinheiro suficiente.&quot;" newline="True"/>
                                                                        </then>
                                                                        <else>
                                                                            <assign variable="Dinheiro[JogadorAtivo]" expression="Dinheiro[JogadorAtivo] - ValorDaCasa"/>
                                                                            <assign variable="NumeroDeCasas[Posicao[JogadorAtivo]]" expression="NumeroDeCasas[Posicao[JogadorAtivo]] + 1"/>
                                                                            <assign variable="Dono[Posicao[JogadorAtivo]]" expression="Jogador"/>
                                                                            <output expression="&quot;Casa adquirida.&quot;" newline="True"/>
                                                                        </else>
                                                                    </if>
                                                                </else>
                                                            </if>
                                                        </then>
                                                        <else>
                                                            <output expression="&quot;0 jogador n&#227;o possui todos os espa&#231;os da cor.&quot;" newline="True"/>
                                                        </else>
                                                    </if>
                                                </then>
                                                <else>
                                                    <output expression="&quot;N&#227;o &#233; poss&#237;vel comprar casa no espa&#231;o indicado.&quot;" newline="True"/>
                                                </else>
                                            </if>
                                        </then>
                                        <else>
                                            <output expression="&quot;N&#227;o &#233; poss&#237;vel comprar casa no espa&#231;o indicado.&quot;" newline="True"/>
                                        </else>
                                    </if>
                                </then>
                                <else>
                                    <output expression="&quot;N&#227;o &#233; a vez do jogador.&quot;" newline="True"/>
                                </else>
                            </if>
                        </then>
                        <else>
                            <output expression="&quot;Jogador n&#227;o participa no jogo em curso.&quot;" newline="True"/>
                        </else>
                    </if>
                </then>
                <else>
                    <output expression="&quot;N&#227;o existe jogo em curso.&quot;" newline="True"/>
                </else>
            </if>
        </body>
    </function>
    <function name="CEComprarEspa&#231;o" type="None" variable="">
        <parameters>
            <parameter name="Jogador" type="String" array="False"/>
            <parameter name="JogoEmProgresso" type="Boolean" array="False"/>
            <parameter name="JogadoresNoJogo" type="String" array="True"/>
            <parameter name="JogadorAtivo" type="Integer" array="False"/>
            <parameter name="Dinheiro" type="Real" array="True"/>
            <parameter name="Tipo" type="String" array="True"/>
            <parameter name="Dono" type="String" array="True"/>
            <parameter name="Preco" type="Real" array="True"/>
            <parameter name="Posicao" type="Integer" array="True"/>
        </parameters>
        <body>
            <if expression="JogoEmProgresso">
                <then>
                    <if expression="EstaNoJogo(Jogador,JogadoresNoJogo)">
                        <then>
                            <if expression="ValidarTurnoDoJogador(Jogador, JogadoresNoJogo, JogadorAtivo)">
                                <then>
                                    <if expression="Tipo[Posicao[JogadorAtivo]] != &quot;Street&quot; &amp;&amp; Tipo[Posicao[JogadorAtivo]] != &quot;Train&quot; &amp;&amp; Tipo[Posicao[JogadorAtivo]] != &quot;Utility&quot;">
                                        <then>
                                            <output expression="&quot;Este espa&#231;o n&#227;o est&#225; para venda.&quot;" newline="True"/>
                                        </then>
                                        <else>
                                            <if expression="Dono[Posicao[JogadorAtivo]] != &quot;&quot;">
                                                <then>
                                                    <output expression="&quot;O espa&#231;o j&#225; se encontra comprado.&quot;" newline="True"/>
                                                </then>
                                                <else>
                                                    <if expression="Dinheiro[JogadorAtivo] &lt; Preco[Posicao[JogadorAtivo]]">
                                                        <then>
                                                            <output expression="&quot;O jogador n&#227;o tem dinheiro suficiente para adquirir o espa&#231;o.&quot;" newline="True"/>
                                                        </then>
                                                        <else>
                                                            <assign variable="Dinheiro[JogadorAtivo]" expression="Dinheiro[JogadorAtivo] - Preco[Posicao[JogadorAtivo]]"/>
                                                            <assign variable="Dono[Posicao[JogadorAtivo]]" expression="JogadoresNoJogo[JogadorAtivo]"/>
                                                            <output expression="&quot;Espa&#231;o comprado.&quot;" newline="True"/>
                                                        </else>
                                                    </if>
                                                </else>
                                            </if>
                                        </else>
                                    </if>
                                </then>
                                <else>
                                    <output expression="&quot;N&#227;o &#233; a vez do jogador.&quot;" newline="True"/>
                                </else>
                            </if>
                        </then>
                        <else>
                            <output expression="&quot;O jogador n&#227;o est&#225; no jogo.&quot;" newline="True"/>
                        </else>
                    </if>
                </then>
                <else>
                    <output expression="&quot;N&#227;o existe um jogo em curso.&quot;" newline="True"/>
                </else>
            </if>
        </body>
    </function>
    <function name="CustoDaCasa" type="Real" variable="CustoDaCasa">
        <parameters>
            <parameter name="Preco" type="Real" array="True"/>
            <parameter name="Posicao" type="Integer" array="True"/>
            <parameter name="JogadorAtivo" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="CustoDaCasa" type="Real" array="False" size=""/>
            <assign variable="CustoDaCasa" expression="Preco[Posicao[JogadorAtivo]] * 0.6"/>
        </body>
    </function>
