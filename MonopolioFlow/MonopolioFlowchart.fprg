    </function>
    <function name="TCTirarCarta" type="None" variable="">
        <parameters>
            <parameter name="Jogador" type="String" array="False"/>
            <parameter name="JogadoresNoJogo" type="String" array="True"/>
            <parameter name="JogoEmProgresso" type="Boolean" array="False"/>
            <parameter name="TemCartaCommunityOuChance" type="Boolean" array="True"/>
            <parameter name="Tipo" type="String" array="True"/>
            <parameter name="Posicao" type="Integer" array="True"/>
            <parameter name="JogadorAtivo" type="Integer" array="False"/>
            <parameter name="Dono" type="String" array="True"/>
            <parameter name="NumeroDeCasa" type="Real" array="True"/>
            <parameter name="Dinheiro" type="Real" array="True"/>
            <parameter name="TurnosNaPrisao" type="Integer" array="True"/>
            <parameter name="EstaNaPrisao" type="Boolean" array="True"/>
            <parameter name="DinheiroFreePark" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="EstaNoJogo" type="Boolean" array="False" size=""/>
            <assign variable="EstaNoJogo" expression="EstaNoJogo(Jogador, JogadoresNoJogo)"/>
            <if expression="JogoEmProgresso == True">
                <then>
                    <if expression="EstaNoJogo == True">
                        <then>
                            <if expression="Jogador == JogadoresNoJogo[JogadorAtivo]">
                                <then>
                                    <if expression="Tipo[Posicao[JogadorAtivo]] == &quot;Community&quot; || Tipo[Posicao[JogadorAtivo]] == &quot;Chance&quot;">
                                        <then>
                                            <if expression="TemCartaCommunityOuChance[JogadorAtivo] == True">
                                                <then>
                                                    <if expression="Tipo[Posicao[JogadorAtivo]] == &quot;Community&quot;">
                                                        <then>
                                                            <call expression="TCTirarCartaCommunity(Dono, Posicao, JogadorAtivo, Dinheiro, NumeroDeCasa, JogadoresNoJogo, DinheiroFreePark)"/>
                                                        </then>
                                                        <else>
                                                            <call expression="TCTirarCartaChance(JogadorAtivo, Dinheiro, Posicao, TurnosNaPrisao, EstaNaPrisao, JogadoresNoJogo, Dono, DinheiroFreePark)"/>
                                                        </else>
                                                    </if>
                                                    <assign variable="TemCartaCommunityOuChance[JogadorAtivo]" expression="False"/>
                                                </then>
                                                <else>
                                                    <output expression="&quot;A Carta j&#225; foi tirada.&quot;" newline="True"/>
                                                </else>
                                            </if>
                                        </then>
                                        <else>
                                            <output expression="&quot;N&#227;o &#233; poss&#237;vel tirar carta neste espa&#231;o.&quot;" newline="True"/>
                                        </else>
                                    </if>
                                </then>
                                <else>
                                    <output expression="&quot;Nao &#233; a vez do Jogador.&quot;" newline="True"/>
                                </else>
                            </if>
                        </then>
                        <else>
                            <output expression="&quot;Jogador n&#227;o participa no jogo em curso.&quot;" newline="True"/>
                        </else>
                    </if>
                </then>
                <else>
                    <output expression="&quot;N&#227;o existe um jogo em curso.&quot;" newline="True"/>
                </else>
            </if>
        </body>
    </function>
    <function name="TCTirarCartaChance" type="None" variable="">
        <parameters>
            <parameter name="JogadorAtivo" type="Integer" array="False"/>
            <parameter name="Dinheiro" type="Real" array="True"/>
            <parameter name="Posicao" type="Integer" array="True"/>
            <parameter name="TurnosNaPrisao" type="Integer" array="True"/>
            <parameter name="EstaNaPrisao" type="Boolean" array="True"/>
            <parameter name="JogadoresNoJogo" type="String" array="True"/>
            <parameter name="Dono" type="String" array="True"/>
            <parameter name="DinheiroFreePark" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="Chance" type="Integer" array="False" size=""/>
            <assign variable="Chance" expression="Random(10)"/>
            <declare name="Mensagem" type="String" array="False" size=""/>
            <if expression="Chance == 1">
                <then>
                    <assign variable="Mensagem" expression="&quot;O jogador recebe 200.&quot;"/>
                    <assign variable="Dinheiro[JogadorAtivo]" expression="Dinheiro[JogadorAtivo] + 200"/>
                </then>
                <else>
                    <if expression="Chance == 2">
                        <then>
                            <assign variable="Mensagem" expression="&quot;O jogador tem de pagar 70.&quot;"/>
                            <if expression="Dinheiro[JogadorAtivo]&lt;70">
                                <then>
                                    <assign variable="Dinheiro[JogadorAtivo]" expression="0"/>
                                    <call expression="VerificarFalencia(JogadorAtivo, Dinheiro, JogadoresNoJogo, Dono)"/>
                                </then>
                                <else>
                                    <assign variable="Dinheiro[JogadorAtivo]" expression="Dinheiro[JogadorAtivo] - 70"/>
                                    <assign variable="DinheiroFreePark" expression="DinheiroFreePark + 70"/>
                                </else>
                            </if>
                        </then>
                        <else>
                            <if expression="Chance == 3 || Chance == 4">
                                <then>
                                    <assign variable="Mensagem" expression="&quot;O jogador recebe 150.&quot;"/>
                                    <assign variable="Dinheiro[JogadorAtivo]" expression="Dinheiro[JogadorAtivo] + 150"/>
                                </then>
                                <else>
                                    <if expression="Chance == 5 || Chance == 6">
                                        <then>
                                            <assign variable="Mensagem" expression="&quot;O jogador move se para a casa start.&quot;"/>
                                            <assign variable="Posicao[JogadorAtivo]" expression="24"/>
                                            <assign variable="Dinheiro[JogadorAtivo]" expression="Dinheiro[JogadorAtivo] + 200"/>
                                        </then>
                                        <else>
                                            <if expression="Chance == 7 || Chance == 8">
                                                <then>
                                                    <assign variable="Mensagem" expression="&quot;O jogador move-se para a casa Police.&quot;"/>
                                                    <assign variable="Posicao[JogadorAtivo]" expression="0"/>
                                                    <assign variable="TurnosNaPrisao[JogadorAtivo]" expression="0"/>
                                                    <assign variable="EstaNaPrisao[JogadorAtivo]" expression="True"/>
                                                </then>
                                                <else>
                                                    <assign variable="Mensagem" expression="&quot;O jogador move-se para a casa FreePark.&quot;"/>
                                                    <assign variable="Posicao[JogadorAtivo]" expression="42"/>
                                                </else>
                                            </if>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
            <output expression="Mensagem" newline="True"/>
        </body>
    </function>
    <function name="TCTirarCartaCommunity" type="None" variable="">
        <parameters>
            <parameter name="Dono" type="String" array="True"/>
            <parameter name="Posicao" type="Integer" array="True"/>
            <parameter name="JogadorAtivo" type="Integer" array="False"/>
            <parameter name="Dinheiro" type="Integer" array="True"/>
            <parameter name="NumeroDeCasa" type="Real" array="True"/>
            <parameter name="JogadoresNoJogo" type="String" array="True"/>
            <parameter name="DinheiroFreePark" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="Chance" type="Integer" array="False" size=""/>
            <assign variable="Chance" expression="Random(10)"/>
            <declare name="Mensagem" type="String" array="False" size=""/>
            <declare name="i" type="Integer" array="False" size=""/>
            <if expression="Chance == 1">
                <then>
                    <declare name="RendaDasCasas" type="Integer" array="False" size=""/>
                    <assign variable="Mensagem" expression="&quot;O jogador paga 20 por cada casa nos seus espa&#231;os.&quot;"/>
                    <declare name="NumCasa" type="Integer" array="False" size=""/>
                    <assign variable="NumCasa" expression="0"/>
                    <for variable="i" start="0" end="Size(Dono) - 1" direction="inc" step="1">
                        <if expression="Dono[i] == JogadoresNoJogo[JogadorAtivo]">
                            <then>
                                <assign variable="NumCasa" expression="NumCasa + NumeroDeCasa"/>
                            </then>
                            <else/>
                        </if>
                    </for>
                    <assign variable="RendaDasCasas" expression="20*NumCasa"/>
                    <if expression="Dinheiro[JogadorAtivo]&lt;RendaDasCasas">
                        <then>
                            <assign variable="Dinheiro[JogadorAtivo]" expression="0"/>
                            <call expression="VerificarFalencia(JogadorAtivo, Dinheiro, JogadoresNoJogo, Dono)"/>
                        </then>
                        <else>
                            <assign variable="Dinheiro[JogadorAtivo]" expression="Dinheiro[JogadorAtivo] - RendaDasCasas"/>
                            <assign variable="DinheiroFreePark" expression="DinheiroFreePark + RendaDasCasas"/>
                        </else>
                    </if>
                </then>
                <else>
                    <if expression="Chance == 2">
                        <then>
                            <assign variable="Mensagem" expression="&quot;O jogador recebe 10 de cada outro jogador.&quot;"/>
                            <for variable="i" start="0" end="Size(JogadoresNoJogo) - 1" direction="inc" step="1">
                                <if expression="JogadoresNoJogo[i] != JogadoresNoJogo[JogadorAtivo]">
                                    <then>
                                        <if expression="Dinheiro[i]&lt;10">
                                            <then>
                                                <assign variable="Dinheiro[i]" expression="0"/>
                                                <call expression="VerificarFalencia(JogadorAtivo, Dinheiro, JogadoresNoJogo, Dono)"/>
                                            </then>
                                            <else>
                                                <assign variable="Dinheiro[i]" expression="Dinheiro[i] - 10"/>
                                                <assign variable="Dinheiro[JogadorAtivo]" expression="Dinheiro[JogadorAtivo] + 10"/>
                                            </else>
                                        </if>
                                    </then>
                                    <else/>
                                </if>
                            </for>
                        </then>
                        <else>
                            <if expression="Chance == 3">
                                <then>
                                    <assign variable="Mensagem" expression="&quot;O jogador tem de pagar 40.&quot;"/>
                                    <if expression="Dinheiro[JogadorAtivo]&lt;40">
                                        <then>
                                            <assign variable="Dinheiro[JogadorAtivo]" expression="0"/>
                                            <call expression="VerificarFalencia(JogadorAtivo, Dinheiro, JogadoresNoJogo, Dono)"/>
                                        </then>
                                        <else>
                                            <assign variable="Dinheiro[JogadorAtivo]" expression="Dinheiro[JogadorAtivo] - 40"/>
                                            <assign variable="DinheiroFreePark" expression="DinheiroFreePark + 40"/>
                                        </else>
                                    </if>
                                </then>
                                <else>
                                    <if expression="Chance == 4">
                                        <then>
                                            <assign variable="Mensagem" expression="&quot;O jogador move-se para Pink1.&quot;"/>
                                            <assign variable="Posicao[JogadorAtivo]" expression="35"/>
                                        </then>
                                        <else>
                                            <if expression="Chance == 5">
                                                <then>
                                                    <assign variable="Mensagem" expression="&quot;O jogador move-se para Teal2&quot;"/>
                                                    <assign variable="Posicao[JogadorAtivo]" expression="25"/>
                                                </then>
                                                <else>
                                                    <if expression="Chance == 6">
                                                        <then>
                                                            <assign variable="Mensagem" expression="&quot;O jogador move-se para White2&quot;"/>
                                                            <assign variable="Posicao[JogadorAtivo]" expression="13"/>
                                                        </then>
                                                        <else>
                                                            <if expression="Chance == 7 || Chance == 8">
                                                                <then>
                                                                    <assign variable="Mensagem" expression="&quot;O jogador recebe 170.&quot;"/>
                                                                    <assign variable="Dinheiro[JogadorAtivo]" expression="Dinheiro[JogadorAtivo] + 170"/>
                                                                </then>
                                                                <else>
                                                                    <assign variable="Mensagem" expression="&quot;O jogador recebe 100.&quot;"/>
                                                                    <assign variable="Dinheiro[JogadorAtivo]" expression="Dinheiro[JogadorAtivo] + 100"/>
                                                                </else>
                                                            </if>
                                                        </else>
                                                    </if>
                                                </else>
                                            </if>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
            <output expression="Mensagem" newline="True"/>
        </body>
    </function>
    <function name="TemTodosOsEspacosCores" type="Boolean" variable="TemTodasAsCores">
        <parameters>
            <parameter name="Jogador" type="String" array="False"/>
            <parameter name="Cor" type="String" array="False"/>
            <parameter name="TipoCor" type="String" array="True"/>
            <parameter name="Dono" type="String" array="True"/>
            <parameter name="Tabuleiro" type="String" array="True"/>
        </parameters>
        <body>
            <declare name="NumeroDeEspacos" type="Integer" array="False" size=""/>
            <assign variable="NumeroDeEspacos" expression="0"/>
            <declare name="EspacosComprados" type="Integer" array="False" size=""/>
            <assign variable="EspacosComprados" expression="0"/>
            <declare name="i" type="Integer" array="False" size=""/>
            <declare name="TemTodasAsCores" type="Boolean" array="False" size=""/>
            <assign variable="TemTodasAsCores" expression="False"/>
            <for variable="i" start="0" end="size(TipoCor)- 1" direction="inc" step="1">
                <if expression="TipoCor[i] == Cor">
                    <then>
                        <assign variable="NumeroDeEspacos" expression="NumeroDeEspacos + 1"/>
                        <if expression="Dono[i] == Jogador">
                            <then>
                                <assign variable="EspacosComprados" expression="EspacosComprados + 1"/>
                            </then>
                            <else/>
                        </if>
                    </then>
                    <else/>
                </if>
            </for>
            <if expression="NumeroDeEspacos == EspacosComprados">
                <then>
                    <assign variable="TemTodasAsCores" expression="True"/>
                </then>
                <else/>
            </if>
        </body>
    </function>
    <function name="TTTerminarTurno" type="Integer" variable="NovoJogadorAtivo">
        <parameters>
            <parameter name="JogoEmProgresso" type="Boolean" array="False"/>
            <parameter name="JogadoresNoJogo" type="String" array="True"/>
            <parameter name="Jogador" type="String" array="False"/>
            <parameter name="JogadorAtivo" type="Integer" array="False"/>
            <parameter name="JaJogouEsteTurno" type="Boolean" array="True"/>
            <parameter name="PrecisaPagarRenda" type="Boolean" array="True"/>
            <parameter name="TemCartaCommunityOuChance" type="Boolean" array="True"/>
            <parameter name="Duplicados" type="Integer" array="True"/>
        </parameters>
        <body>
            <declare name="NovoJogadorAtivo" type="Integer" array="False" size=""/>
            <assign variable="NovoJogadorAtivo" expression="JogadorAtivo"/>
            <if expression="JogoEmProgresso">
                <then>
                    <if expression="ValidarTurnoDoJogador(Jogador, JogadoresNoJogo, JogadorAtivo)">
                        <then>
                            <if expression="JaJogouEsteTurno[JogadorAtivo] == False || PrecisaPagarRenda[JogadorAtivo] == True || TemCartaCommunityOuChance[JogadorAtivo] == True || (Duplicados[JogadorAtivo] &gt; 0 &amp;&amp; Duplicados[JogadorAtivo] &lt; 2)">
                                <then>
                                    <output expression="&quot;O jogador ainda tem a&#231;&#245;es a fazer.&quot;" newline="True"/>
                                </then>
                                <else>
                                    <assign variable="JaJogouEsteTurno[JogadorAtivo]" expression="False"/>
                                    <assign variable="PrecisaPagarRenda[JogadorAtivo]" expression="False"/>
                                    <assign variable="TemCartaCommunityOuChance[JogadorAtivo]" expression="False"/>
                                    <assign variable="Duplicados[JogadorAtivo]" expression="0"/>
                                    <assign variable="JogadorAtivo" expression="(JogadorAtivo + 1) % size(JogadoresNoJogo)"/>
                                    <assign variable="NovoJogadorAtivo" expression="JogadorAtivo"/>
                                    <while expression="JogadoresNoJogo[JogadorAtivo] == &quot;&quot;">
                                        <assign variable="JogadorAtivo" expression="(JogadorAtivo + 1) % size(JogadoresNoJogo)"/>
                                    </while>
                                    <declare name="NomeProximoJogador" type="String" array="False" size=""/>
                                    <assign variable="NomeProximoJogador" expression="JogadoresNoJogo[JogadorAtivo]"/>
                                    <output expression="&quot;Turno terminado. Novo turno do jogador &quot; &amp; NomeProximoJogador &amp; &quot;.&quot;" newline="True"/>
                                </else>
                            </if>
                        </then>
                        <else>
                            <output expression="&quot;N&#227;o &#233; o turno do jogador indicado&quot;" newline="True"/>
                        </else>
                    </if>
                </then>
                <else>
                    <output expression="&quot;N&#227;o existe jogo em curso.&quot;" newline="True"/>
                </else>
            </if>
        </body>
    </function>
    <function name="ValidarTurnoDoJogador" type="Boolean" variable="eJogadorAtivo">
        <parameters>
            <parameter name="Jogador" type="String" array="False"/>
            <parameter name="JogadoresNoJogo" type="String" array="True"/>
            <parameter name="JogadorAtivo" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="eJogadorAtivo" type="Boolean" array="False" size=""/>
            <assign variable="eJogadorAtivo" expression="False"/>
            <if expression="Jogador == JogadoresNoJogo[JogadorAtivo]">
                <then>
                    <assign variable="eJogadorAtivo" expression="True"/>
                </then>
                <else/>
            </if>
        </body>
    </function>
    <function name="VerificarFalencia" type="None" variable="">
        <parameters>
            <parameter name="JogadorAtivo" type="Integer" array="False"/>
            <parameter name="Dinheiro" type="Real" array="True"/>
            <parameter name="JogadoresNoJogo" type="String" array="True"/>
            <parameter name="Dono" type="String" array="True"/>
            <parameter name="Tabuleiro" type="String" array="True"/>
        </parameters>
        <body>
            <if expression="Dinheiro[JogadorAtivo] &lt;= 0">
                <then>
                    <assign variable="JogadoresNoJogo[JogadorAtivo]" expression="&quot;&quot;"/>
                </then>
                <else/>
            </if>
        </body>
    </function>
</flowgorithm>
