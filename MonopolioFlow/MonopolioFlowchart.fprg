            <if expression="JogoEmProgresso = True">
                <then>
                    <declare name="i" type="Integer" array="False" size=""/>
                    <for variable="i" start="0" end="Size(Tabuleiro) - 1" direction="inc" step="1">
                        <output expression="&quot;| &quot; &amp; Tabuleiro[i] &amp; &quot; &quot;" newline="False"/>
                        <if expression="Dono[i] != &quot;&quot;">
                            <then>
                                <if expression="NumeroDeCasas[i] &gt; 0">
                                    <then>
                                        <output expression="&quot;(&quot; &amp; Dono[i] &amp; &quot; - &quot; &amp; NumeroDeCasas[i] &amp; &quot; casas) &quot;" newline="False"/>
                                    </then>
                                    <else>
                                        <output expression="&quot;(&quot; &amp; Dono[i] &amp; &quot;) &quot;" newline="False"/>
                                    </else>
                                </if>
                            </then>
                            <else>
                                <output expression="&quot; &quot;" newline="False"/>
                            </else>
                        </if>
                        <declare name="j" type="Integer" array="False" size=""/>
                        <for variable="j" start="0" end="Size(Posicao) - 1" direction="inc" step="1">
                            <if expression="Posicao[j] = i">
                                <then>
                                    <output expression="Jogadores[j] &amp; &quot; &quot;" newline="False"/>
                                </then>
                                <else/>
                            </if>
                        </for>
                        <if expression="(i + 1) % 7 = 0">
                            <then>
                                <output expression="&quot;|&quot;" newline="True"/>
                            </then>
                            <else/>
                        </if>
                    </for>
                    <output expression="Jogadores[JogadorAtivo] &amp; &quot; - &quot; &amp; Dinheiro[JogadorAtivo]" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;N&#227;o existe jogo em curso&quot;" newline="True"/>
                </else>
            </if>
        </body>
